resource "aws_security_group" "web" {
   name = "mysql"
  ingress {
 from_port = local.ssh_port
 to_port = local.ssh_port
 cidr_blocks = [ local.anywhere ]
 protocol = local.tcp
  }
  ingress {
 from_port = local.http_port
 to_port = local.http_port
 cidr_blocks = [ local.anywhere ]
 protocol = local.tcp
  }
  tags = {
    Name = "web"
  }
  vpc_id = local.vpc_id
  depends_on = [ aws_subnet.subnets ]
}
